{
  "plugins": [
    {
      "name": "phovea_core",
      "type": "lib",
      "description": "Phovea Core Plugin",
      "repository": "https://github.com/phovea/phovea_core.git",
      "dependencies": {
        "phovea_core": "github:phovea/phovea_core"
      },
      "libraries": []
    },
    {
      "name": "phovea_d3",
      "type": "lib",
      "description": "Phovea D3 Plugin",
      "repository": "https://github.com/phovea/phovea_d3.git",
      "dependencies": {
        "phovea_d3": "github:phovea/phovea_d3"
      },
      "libraries": [
        "d3"
      ]
    },
    {
      "name": "phovea_vis",
      "type": "lib",
      "description": "Phovea Vis Plugin",
      "repository": "https://github.com/phovea/phovea_vis.git",
      "dependencies": {
        "phovea_vis": "github:phovea/phovea_vis"
      },
      "libraries": [
        "d3"
      ]
    },
    {
      "name": "phovea_bootstrap_fontawesome",
      "type": "lib",
      "description": "Phovea Common Style Plugin",
      "repository": "https://github.com/phovea/phovea_bootstrap_fontawesome.git",
      "dependencies": {
        "phovea_bootstrap_fontawesome": "github:phovea/phovea_bootstrap_fontawesome"
      },
      "libraries": [
        "jquery",
        "font-awesome"
      ]
    },
    {
      "name": "phovea_importer",
      "type": "lib",
      "description": "Phovea Importer Plugin",
      "repository": "https://github.com/phovea/phovea_importer.git",
      "dependencies": {
        "phovea_importer": "github:phovea/phovea_importer"
      },
      "libraries": [
        "d3",
        "papaparse"
      ]
    },
    {
      "name": "phovea_vis_lineup",
      "type": "lib",
      "description": "Phovea LineUp Visualization",
      "repository": "https://github.com/phovea/phovea_vis_lineup.git",
      "dependencies": {
        "phovea_vis_lineup": "github:phovea/phovea_vis_lineup"
      },
      "libraries": [
        "lineupjs",
        "d3",
        "font-awesome"
      ]
    },
    {
      "name": "phovea_server",
      "type": "service",
      "description": "Phovea Server Plugin",
      "repository": "https://github.com/phovea/phovea_server.git",
      "requirements": {
        "-e git+https://github.com/phovea/phovea_server.git": "#egg=phovea_server"
      },
      "debianPackages": {
      },
      "redhatPackages": {
      }
    }
  ],
  "libraries": [
    {
      "name": "d3",
      "type": "web",
      "description": "D3 v3 Library",
      "aliases": {
        "d3": "d3/d3"
      },
      "dependencies": {
        "@types/d3": "^3.5.36",
        "d3": "^3.5.17"
      }
    },
    {
      "name": "jquery",
      "type": "web",
      "description": "JQuery 3.1",
      "aliases": {},
      "dependencies": {
        "@types/jquery": "^2.0.33",
        "jquery": "^3.1.1"
      }
    },
    {
      "name": "papaparse",
      "type": "web",
      "description": "PapaParse",
      "aliases": {},
      "dependencies": {
        "@types/papaparse": "^4.1.28",
        "papaparse": "^4.1.2"
      }
    },
    {
      "name": "jqueryui",
      "type": "web",
      "description": "JQuery UI",
      "aliases": {},
      "dependencies": {
        "@types/jqueryui": "^1.11.31",
        "jqueryui": "^1.11.1"
      }
    },
    {
      "name": "lineupjs",
      "type": "web",
      "description": "LineUp.js",
      "aliases": {},
      "dependencies": {
        "lineupjs": "github:Caleydo/lineup.js#feature_146_react"
      }
    },
    {
      "name": "font-awesome",
      "type": "web",
      "description": "Font Awesome",
      "aliases": {},
      "dependencies": {
        "font-awesome": "^4.6.3"
      }
    },
    {
      "name": "bootstrap",
      "type": "web",
      "description": "Bootstrap 3",
      "aliases": {},
      "externals": ["bootstrap-sass"],
      "dependencies": {
        "@types/bootstrap": "^3.3.32",
        "bootstrap-sass": "^3.3.7"
      }
    },
    {
      "name": "redis",
      "type": "python",
      "description": "Redis Server",
      "requirements": {
        "redis": "==2.10.5"
      },
      "debianPackages": {
        "redis-server": ""
      },
      "redhatPackages": {
        "redis": ""
      }
    },
    {
      "name": "mongodb",
      "type": "python",
      "description": "Mongo DB",
      "requirements": {
        "pymongo": "==3.0.3"
      },
      "debianPackages": {
        "mongodb": ""
      },
      "redhatPackages": {
        "mongodb-org-server": ""
      }
    }
  ]
}
