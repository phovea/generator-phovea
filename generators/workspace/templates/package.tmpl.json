{
  "name": "<%-wsName%>",
  "description": "<%-wsDescription%>",
  "version": "<%-wsVersion%>",
  "author": {
    "name": "Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "engines": {
    "npm": ">=6.12",
    "node": ">= 14.19"
  },
  "scripts": {
    "preinstall": "rimraf node_modules package-lock.json || exit 0",
    "postinstall": "npm-run-all --parallel delete-dependencies:*",
    "all:compile": "npm-run-all --parallel compile:*",
    "all:compile:watch": "npm-run-all --parallel compile:watch:*",
    "all:copy": "npm-run-all --parallel copy:*",
    "all:copy:watch": "npm-watch all:copy",
    "build:dev:watch": "webpack --mode development --config config/webpack.dev.js --watch",
    "build:dev": "webpack --mode development --config config/webpack.dev.js",
    "build": "webpack --mode production --config config/webpack.prod.js",
    "bundle-report": "webpack-bundle-analyzer --port 4300 bundles/stats.json",
    "webpack:dev": "webpack-dev-server --mode development --config config/webpack.dev.js",
    "webpack:dev:watch": "npm run webpack:dev",
    "start:watch:all": "npm-run-all --parallel all:compile:watch all:copy:watch webpack:dev:watch",
    "start:watch:dev-repos": "npm-run-all --parallel dev-repos:compile:watch dev-repos:copy:watch webpack:dev:watch",
    "start": "npm run start:watch:dev-repos",
    "predist": "npm run build",
    "dist": "mkdir dist && cd bundles && tar cvzf ../dist/bundles.tar.gz *"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@webpack-cli/serve": "1.6.1",
    "clean-webpack-plugin": "~3.0.0",
    "copy-webpack-plugin": "~6.3.2",
    "css-loader": "~4.3.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "dotenv-webpack": "^5.0.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "expose-loader": "~1.0.3",
    "file-loader": "~6.2.0",
    "html-loader": "~1.3.2",
    "html-webpack-plugin": "~4.5.0",
    "identity-obj-proxy": "~3.0.0",
    "ifdef-loader": "~2.1.5",
    "imports-loader": "~1.2.0",
    "mini-css-extract-plugin": "~1.3.2",
    "npm-run-all": "~4.1.5",
    "npm-watch": "^0.7.0",
    "prettier": "^2.5.1",
    "raw-loader": "~4.0.2",
    "rimraf": "~3.0.2",
    "sass": "1.52.1",
    "sass-loader": "~9.0.2",
    "shx": "~0.3.3",
    "source-map-loader": "~1.1.3",
    "style-loader": "~2.0.0",
    "tslib": "~2.3.1",
    "typedoc": "~0.22.13",
    "typescript": "~4.6.2",
    "url-loader": "~4.1.1",
    "webpack": "~4.46.0",
    "webpack-bundle-analyzer": "~4.5.0",
    "webpack-cli": "~4.10.0",
    "webpack-dev-server": "~4.7.4"
  }
}
