{
  "name": "<%-wsName%>",
  "description": "<%-wsDescription%>",
  "version": "<%-wsVersion%>",
  "author": {
    "name": "Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "engines": {
    "npm": ">=6.12",
    "node": ">= 12.13"
  },
  "scripts": {
    "preinstall": "rimraf node_modules package-lock.json || exit 0",
    "postinstall": "npm-run-all --parallel delete-dependencies:*",
    "all:compile": "npm-run-all --parallel compile:*",
    "all:compile:watch": "npm-run-all --parallel compile:watch:*",
    "all:copy": "npm-run-all --parallel copy:*",
    "all:copy:watch": "npm-watch all:copy",
    "build:dev:watch": "webpack --mode development --config config/webpack.dev.js --watch",
    "build:dev": "webpack --mode development --config config/webpack.dev.js",
    "build": "webpack --mode production --config config/webpack.prod.js",
    "bundle-report": "webpack-bundle-analyzer --port 4300 bundles/stats.json",
    "webpack:dev": "webpack-dev-server --mode development --config config/webpack.dev.js",
    "webpack:dev:watch": "npm run webpack:dev",
    "start:watch": "npm-run-all --parallel all:copy:watch webpack:dev:watch",
    "start": "npm run start:watch",
    "predist": "npm run build",
    "dist": "mkdir dist && cd bundles && tar cvzf ../dist/bundles.tar.gz *"
  },
  "devDependencies": {
    "@babel/runtime": "~7.16.3",
    "@babel/plugin-proposal-class-properties": "~7.16.0",
    "@babel/plugin-proposal-decorators": "~7.16.4",
    "@babel/plugin-transform-runtime": "~7.16.4",
    "@babel/preset-env": "~7.16.4",
    "@babel/preset-react": "~7.16.0",
    "@babel/preset-typescript": "~7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "~0.5.2",
    "babel-loader": "~8.2.3",
    "clean-webpack-plugin": "~3.0.0",
    "copy-webpack-plugin": "~6.3.2",
    "css-loader": "~4.3.0",
    "css-minimizer-webpack-plugin": "~1.1.5",
    "dotenv-webpack": "~5.0.0",
    "expose-loader": "~1.0.3",
    "file-loader": "~6.2.0",
    "fork-ts-checker-webpack-plugin": "~6.4.0",
    "html-loader": "~1.3.2",
    "html-webpack-plugin": "~4.5.0",
    "identity-obj-proxy": "~3.0.0",
    "ifdef-loader": "~2.1.5",
    "imports-loader": "~1.2.0",
    "mini-css-extract-plugin": "~1.3.2",
    "npm-run-all": "~4.1.5",
    "npm-watch": "~0.7.0",
    "raw-loader": "~4.0.2",
    "react-refresh": "~0.11.0",
    "rimraf": "~3.0.2",
    "sass": "~1.29.0",
    "sass-loader": "~9.0.2",
    "shx": "~0.3.3",
    "source-map-loader": "~1.1.3",
    "speed-measure-webpack-plugin": "~1.5.0",
    "tslib": "~2.1.0",
    "tslint": "~5.20.1",
    "typedoc": "~0.19.2",
    "typescript": "~3.9.7",
    "url-loader": "~4.1.1",
    "webpack": "~4.46.0",
    "webpack-bundle-analyzer": "~3.9.0",
    "webpack-cli": "~4.9.1",
    "webpack-dev-server": "~4.5.0"
  }
}
