{
  "name": "<%-wsName%>",
  "description": "<%-wsDescription%>",
  "version": "<%-wsVersion%>",
  "author": {
    "name": "Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "engines": {
    "npm": ">=6.12",
    "node": ">= 12.13"
  },
  "scripts": {
    "preinstall": "rimraf node_modules package-lock.json || exit 0",
    "postinstall": "npm-run-all --parallel delete-dependencies:*",
    "all:compile": "npm-run-all --parallel compile:*",
    "all:compile:watch": "npm-run-all --parallel compile:watch:*",
    "all:copy": "npm-run-all --parallel copy:*",
    "all:copy:watch": "npm-watch all:copy",
    "build:dev:watch": "webpack --mode development --config ./webpack.config.ts --watch",
    "build:dev": "webpack --mode development --config ./webpack.config.ts",
    "build": "webpack --mode production --config ./webpack.config.ts",
    "bundle-report": "webpack-bundle-analyzer --port 4300 bundles/stats.json",
    "webpack:dev": "webpack serve --mode development --config ./webpack.config.ts --progress",
    "start": "npm-run-all --parallel all:copy:watch webpack:dev",
    "predist": "npm run build",
    "dist": "mkdir dist && cd bundles && tar cvzf ../dist/bundles.tar.gz *"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@svgr/webpack": "^6.2.1",
    "@types/bootstrap": "^5.0.17",
    "@types/jquery": "^3.5.5",
    "@types/node": "^17.0.24",
    "@types/react-dom": "^16.9.14",
    "@types/react": "^16.14.25",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv-webpack": "^7.1.0",
    "eslint-webpack-plugin": "^3.1.1",
    "expose-loader": "^3.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.6",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "~3.0.0",
    "ifdef-loader": "^2.3.2",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "~4.1.5",
    "npm-watch": "~0.11.0",
    "react-refresh": "^0.12.0",
    "rimraf": "~3.0.2",
    "sass-loader": "^12.6.0",
    "sass": "^1.50.0",
    "shx": "~0.3.3",
    "source-map-loader": "^3.0.1",
    "speed-measure-webpack-plugin": "~1.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-node": "^10.7.0",
    "tslib": "~2.3.1",
    "type-fest": "^2.12.2",
    "typedoc": "~0.22.13",
    "typescript": "~4.6.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack": "^5.72.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9"
  }
}
