{
  "name": "generator-phovea",
  "description": "helper generator for phovea",
  "homepage": "https://github.com/phovea/generator-phovea",
  "version": "0.1.0",
  "author": {
    "name": "Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/phovea/generator-phovea/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phovea/generator-phovea.git"
  },
  "main": "generators/app/index.js",
  "files": [
    "generators/",
    "utils/",
    "knownPhoveaPlugins.json"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "escodegen": "^1.11.0",
    "esprima": "^4.0.1",
    "esprima-walk": "^0.1.0",
    "fs-extra": "^7.0.1",
    "git-remote-origin-url": "^2.0.0",
    "glob": "^7.1.3",
    "inquirer": "^6.2.1",
    "inquirer-npm-name": "^3.0.0",
    "lodash": "^4.17.11",
    "opn": "^5.4.0",
    "parse-author": "2.0.0",
    "request": "^2.88.0",
    "semver": "^5.6.0",
    "semver-intersect": "^1.4.0",
    "yamljs": "0.3.0",
    "yeoman-environment": "2.3.4",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "eslint": "5.10.0",
    "eslint-config-xo-space": "0.20.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "5.0.0",
    "gulp-exclude-gitignore": "^1.2.0",
    "gulp-istanbul": "^1.1.3",
    "gulp-line-ending-corrector": "1.0.3",
    "gulp-mocha": "^6.0.0",
    "gulp-nsp": "3.0.1",
    "gulp-plumber": "1.2.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    },
    "rules": {
      "linebreak-style": 0,
      "valid-jsdoc": 0,
      "capitalized-comments": 0,
      "no-div-regex": 0,
      "padded-blocks": 0,
      "arrow-parens": [
        2,
        "always"
      ]
    }
  },
  "scripts": {
    "lint": "gulp static",
    "prepublishOnly": "gulp prepublish",
    "test": "gulp",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  }
}
