{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "linebreak-style": 0,
    "valid-jsdoc": 0,
    "capitalized-comments": 0,
    "no-div-regex": 0,
    "padded-blocks": 0,
    "arrow-parens": [
      2,
      "always"
    ]
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  }
}
